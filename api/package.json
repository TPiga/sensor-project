{
  "name": "ChallengeProject",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nodemon src/main.ts",
    "start:debug": "nodemon --config nodemon-debug.json",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "ts-node -r dotenv-safe/config node_modules/.bin/jest * dotenv_config_path=.env.test --maxWorkers=4",
    "test:watch": "jest --watch",
    "test:coverage": "ts-node -r dotenv-safe/config node_modules/.bin/jest * --coverage dotenv_config_path=.env.test --maxWorkers=4",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest",
    "test:e2e": "jest --config ./test/jest-e2e.json --maxWorkers=4"
  },
  "dependencies": {
    "@koa/cors": "2",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/koa": "^2.0.48",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-router": "^7.0.40",
    "@types/koa__cors": "^2.2.3",
    "axios": "^0.19.0",
    "cookie-parser": "^1.4.4",
    "dayjs": "^1.8.14",
    "dotenv-safe": "^6.1.0",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-joi-router": "^6.0.0",
    "koa-jwt": "^3.5.1",
    "koa-router": "^7.4.0",
    "lodash": "^4.17.11",
    "redis": "^2.8.0",
    "typescript": "^3.0.1",
    "uuid": "^3.3.2",
    "uuid-validate": "^0.0.3",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^3.9.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cookie-parser": "^1.4.1",
    "@types/dotenv-safe": "^5.0.3",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.1",
    "@types/lodash": "^4.14.121",
    "@types/node": "^12.0.5",
    "@types/supertest": "^2.0.5",
    "jest": "^23.5.0",
    "nodemon": "^1.18.3",
    "prettier": "^1.14.2",
    "supertest": "^3.1.0",
    "ts-jest": "^23.1.3",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.5.0",
    "tslint": "5.11.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
